{
  "name": "pwa-experiments",
  "private": "true",
  "version": "0.1.9",
  "description": "pwa experiments with Vue.js and material-components-web",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matsp/pwa-experiments.git"
  },
  "homepage": "https://matsp.github.io/pwa-experiments",
  "scripts": {
    "lint": "eslint --ext .js --ext .vue --fix ./",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build:prod": "webpack --config webpack.config.prod.js",
    "serve:dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.dev.js",
    "serve:prod": "yarn build:prod && cd ./public && python -m http.server 8080",
    "release": "standard-version"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "material-components-vue": "^0.11.1",
    "normalize.css": "^8.0.0",
